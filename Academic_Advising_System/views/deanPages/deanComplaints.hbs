
<div class="main-content" name="main-content" id="main-content">
    <div class="content">
        <div class="body-header">
            <header>
                <h1><span class="trn">Complaints</span></h1>
            </header>
            <hr>
        </div>
        <div class="body-content">
             <div id="ResevedMSG">
              {{#each complist}} 
                            {{#each this}}     
                                {{#if this.compfrom}}
                                    <div class="complcontainer" style="background:lightblue;">
