<div class="main-content" name="main-content" id="main-content">
        <div class="content">
            <div class="body-header">
                <header>
                    <h1><span class="trn">Personal Profile</span></h1>
                </header>
                <hr>
            </div>
            <!--        <div class="body-content">-->
            <!-- Student Profile -->
            <div class="student-profile py-4">
                <div class="Profilecontainer">
                    <div class="row">
                        <div class="col-lg-12">
