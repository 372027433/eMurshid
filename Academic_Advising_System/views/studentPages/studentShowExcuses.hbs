                                <div class="card-header bg-transparent border-0">
                                    <h3 class="mb-0"><i class="far fa-clone pr-1"></i>General Information</h3>

                                    <table>
                                        <tr>
                                        <th>Excuse ID </th>
                                        <th>Submission Date</th>
                                        <th>Type</th>
                                        <th>Status</th>
                                        </tr>
                                    <tr>
                                        <td>{{excuseId}} </td>
                                        <td> {{submissionDate}}</td>
                                        <td> {{type}} </td>
                                        <td> {{status}}</td>
                                    </tr>
                                    </table>
                                </div>
                                <div class="card-body pt-0">

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

